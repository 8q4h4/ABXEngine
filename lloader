Toggle.MouseButton1Down:Connect(function()
	if Toggle.Text == "X" then
		Toggle.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		Toggle.Text = ""
	else
		Toggle.BackgroundColor3 = registeredLoaders[getfenv().currentLoader].Color
		Toggle.Text = "X"
	end
end)

